<div class="allBeers">
    <a href="/beer/{{this.id}}">
        <li>
            <div>
                <img src="{{this.image_url}}" alt="">
            </div>
            <p>
                <b>{{this.name}}</b>
            </p>
            <p class="tagLine">
                {{this.tagline}}
            </p>
            <p>
                {{this.description}}
            </p>

        </li>
    </a>
</div>